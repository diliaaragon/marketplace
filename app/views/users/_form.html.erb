<%= simple_form_for @user do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.input :cellphone %>
  <%= f.input :address %>
  <%= f.button :submit, class:"btn btn-primary" %>
  <%= link_to 'Back', users_path, class:"btn btn-primary" %>
  <br>
<% end %>