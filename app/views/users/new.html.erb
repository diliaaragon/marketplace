<div class="container">
  <h1 class="text-info"> Create a New Account </h1><br>
  <%= simple_form_for @user do |f| %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
    <%= f.input :cellphone %>
    <%= f.input :address %>
    <%= f.button :submit, class:"btn btn-primary"  %> <%= link_to 'Back', users_path, class:"btn btn-primary" %>
    <br>
  <% end %>
</div>